{
    "data_dir": "../data",
    "result_dir": "./result/",
    "config_dir": "./exp_config/",
    "pretrained_model": "beomi/beep-koelectra-base-v3-discriminator-hate",  
    "architecture": "ElectraForSequenceClassification",
    "tokenizer_class": "ElectraTokenizer",
    "num_classes": 3,
    "max_seq_len": 137,
    "train_epochs": 50,
    "adam_epsilon": 1e-8,
    "seed": 42,
    "train_batch_size": 16,
    "eval_batch_size": 16,
    "learning_rate": 1e-6,
    "warmup_proportion": 0.15,
    "run": "beomi_koelectra_v3_hate",
    "patience": 3
}
