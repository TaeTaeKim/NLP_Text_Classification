{
    "data_dir": "./data",
    "result_dir": "./result/",
    "config_dir": "./exp_config/",
    "pretrained_model": "skt/kobert-base-v1",  
    "architecture": "BertForSequenceClassification",
    "tokenizer_class": "Kobert",
    "num_classes": 6,
    "max_seq_len": 128,
    "train_epochs": 10,
    "adam_epsilon": 1e-8,
    "seed": 42,
    "train_batch_size": 32,
    "eval_batch_size": 32,
    "learning_rate": 1e-6,
    "warmup_proportion": 0.1,
    "run": "EDA_Kobert",
    "patience": 3
}
